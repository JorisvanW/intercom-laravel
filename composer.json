{
    "name": "jorisvanw/intercom-laravel",
    "description": "Laravel Intercom PHP wrapper",
    "keywords": [
        "intercom",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joris v. Willigen",
            "email": "me@jorisvanw.me"
        },
        {
            "name": "Alex Slaughter",
            "email": "alex@alexslaughter.com"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "intercom/intercom-php": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Shadow\\IntercomLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shadow\\IntercomLaravel\\ServiceProvider"
            ],
            "aliases": {
                "Intercom": "Shadow\\IntercomLaravel\\Facade"
            }
        }
    }
}
